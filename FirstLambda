import json
import datetime
import boto3
lambda_client = boto3.client('lambda')
                     
                     
def lambda_handler(event, context):
    # TODO implement
    lambda_payload = json.dumps(event['queryStringParameters']) # Extract only get parameter
    #lambda_payload = json.dumps("Hello")
    
    lambda_client.invoke(FunctionName='YourLambdaName',
                     InvocationType='Event',
                     Payload=lambda_payload)
    

    return {
        'statusCode': 200,
        'body': json.dumps(event['queryStringParameters'])
    }
